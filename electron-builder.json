{
    "appId": "com.myflomo.app",
    "productName": "MyFlomo",
    "directories": {
        "output": "release"
    },
    "files": [
        "dist/**/*",
        "electron/**/*"
    ],
    "mac": {
        "target": [
            "dmg"
        ],
        "icon": "public/icon.icns"
    },
    "win": {
        "target": [
            "nsis"
        ],
        "icon": "public/icon.ico"
    },
    "linux": {
        "target": [
            "AppImage"
        ],
        "icon": "public/icon.png"
    }
}